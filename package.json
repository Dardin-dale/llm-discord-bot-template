{
  "name": "discord-bot-template",
  "version": "1.0.0",
  "description": "Modular Discord bot template for AWS Lambda - optimized for Claude Code",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "dev": "nodemon --exec ts-node scripts/local-server.ts",
    "predeploy": "ts-node scripts/pre-deploy.ts",
    "deploy": "cdk deploy BotStack",
    "destroy": "cdk destroy BotStack",
    "register": "ts-node scripts/register-commands.ts",
    "validate": "ts-node scripts/validate.ts",
    "validate:env": "ts-node scripts/pre-deploy.ts",
    "synth": "cdk synth"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.17.0",
    "aws-cdk": "^2.198.0",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@aws-sdk/client-lambda": "^3.600.0",
    "@aws-sdk/client-ssm": "^3.600.0",
    "@discordjs/builders": "^1.10.1",
    "@discordjs/rest": "^2.4.3",
    "@google/generative-ai": "^0.21.0",
    "aws-cdk-lib": "^2.198.0",
    "constructs": "^10.4.0",
    "discord-api-types": "^0.37.120",
    "discord-interactions": "^4.3.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.0",
    "source-map-support": "^0.5.21"
  }
}
